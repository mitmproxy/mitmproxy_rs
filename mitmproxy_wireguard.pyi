from typing import Awaitable, Callable, Optional

class WireGuardServer:
    def getsockname(self) -> tuple[str, int]: ...
    def send_datagram(self, data: bytes, src_addr: tuple[str, int], dst_addr: tuple[str, int]) -> None: ...
    def close(self) -> None: ...
    async def wait_closed(self) -> None: ...

class WireGuardServerConf:
    @staticmethod
    def new(*, name: str = "mitmproxy_wireguard", listen_port: int = 51820, peers: int = 1) -> WireGuardServerConf: ...
    @staticmethod
    def load(name: str) -> WireGuardServerConf: ...
    @staticmethod
    def custom(listen_port: int, server_private_key: str, peer_keys: list[str]) -> WireGuardServerConf: ...

class TcpStream:
    async def read(self, n: int) -> bytes: ...
    def write(self, data: bytes): ...
    async def drain(self) -> None: ...
    def write_eof(self): ...
    def close(self): ...
    def get_extra_info(self, name: str) -> tuple[str, int]: ...
    def __repr__(self) -> str: ...

class UdpStream:
    async def read(self, n: int) -> bytes: ...
    def write(self, data: bytes): ...
    async def drain(self) -> None: ...
    def write_eof(self): ...
    def close(self): ...
    def get_extra_info(self, name: str) -> tuple[str, int]: ...
    def __repr__(self) -> str: ...

async def start_server(
    host: str,
    conf: WireGuardServerConf,
    handle_connection: Callable[[TcpStream], Awaitable[None]],
    receive_datagram: Callable[[UdpStream], Awaitable[None]],
) -> WireGuardServer: ...
